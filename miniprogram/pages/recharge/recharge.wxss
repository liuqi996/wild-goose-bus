/* pages/recharge/recharge.wxss */
@import "../../file/ui/icon.wxss";
.map{
  width:100%;
  height: 60vh;
}
.content{
  width: 100%;
  background-color: #fff;
}
.shengming{
  margin-top: 10px;
  margin-left:15px;
  width: 60px;
  height: 18px;
  color: rgb(126, 124, 124);
  font-size: 11px;
  background-color: rgb(252, 220, 210);
  /* background-color: rgb(255, 193, 174); */
  /* background-color: rgb(45, 190, 1); */
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 3px;
}
.czd{
  /* margin-top:10px; */
}
.czd_item{
  width: 92%;
  margin-left:4%;
  margin-top:10px;
  /* margin-top: 18px; */
  margin-bottom: 8px;
  padding:0px 0 5px 5px;
  border-bottom:0.6px solid #919191;
}
.top_line{
  display: flex;
  flex-flow : row;
  justify-content: space-between;
  align-items: baseline;
}
.title{
  font-size: 15px;
  font-weight: 700;
  letter-spacing: 1px;
  color:#2B98F0;
}
.meter{
  font-size: 13px;
  color:#66BA4E;
  background-color: #f0f0f0;
  padding: 2px 3px 2px 3px;
  border-radius: 3px
}
.address{
  margin-top: 2px;
  font-size: 13px;
}